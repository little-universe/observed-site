    "flow-bin": "^0.69.0",
    "glow": "^1.2.2",
    "html-webpack-plugin": "^3.1.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.4",
    "pre-commit": "^1.2.2",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.7",
    "serve": "^10.1.2",
    "style-loader": "^0.20.3",
